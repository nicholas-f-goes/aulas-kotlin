<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nicholas.goes.helloworldproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HelloWorldProject">
        <activity android:name=".WorkActivity"></activity>
        <activity android:name=".HistoricActivity" />
        <activity android:name=".TimesheetActivity" />
        <activity android:name=".NoteActivity" />
        <activity android:name=".CertificateActivity" />
        <activity android:name=".BoletoActivity" />
        <activity android:name=".AboutActivity" />
        <activity
            android:name=".SplashActivity"
            android:immersive="true"
            android:theme="@style/Theme.HelloWorld.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
    </application>

</manifest>